package com.app;

import java.util.*;

public class AutoBoxingAndUnboxingDemo {
	public static void main(String[] args) {
		Integer x = new Integer(10);
		Integer y = x;
		System.out.println(x == y);// true
		
		System.out.println("-----------------------------");
		Integer a=10;
		Integer b=10;
		System.out.println(a==b);//true
	}
}
